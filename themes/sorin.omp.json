{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "exit",
          "style": "plain",
          "foreground": "#CB4B16"
        },
        {
          "type": "root",
          "style": "plain",
          "foreground": "#CECE04"
        },
        {
          "type": "session",
          "style": "plain",
          "foreground": "#FFFFFF"
        },
        {
          "type": "path",
          "style": "plain",
          "foreground": "#0973C0",
          "properties": {
            "style": "full",
            "prefix": ""
          }
        },
        {
          "type": "git",
          "style": "plain",
          "foreground": "#C1C106",
          "properties": {
            "prefix": "git:"
          }
        },
        {
          "type": "python",
          "style": "plain",
          "foreground": "#100e23",
          "properties": {
            "prefix": " \uE235 "
          }
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "#ffffff",
          "properties": {
            "prefix": "",
            "text": "\u276F\u276F\u276F"
          }
        }
      ]
    }
  ]
}
